// 每道菜连续点的人数
select max(cnt) as mcnt, count(*), sum(cnt), dish_name as mcnt  from (select phone, dish_name, count(*) as cnt from user_menu_info where dish_name>"" and dish_name not like "%米饭%" and dish_name not like "%一次性%" group by phone, dish_name having cnt >1 order by cnt  desc limit 1000) as e group by dish_name order by mcnt desc ;

