                <li><a href="/login.html"><i class="icon-off"></i>登出</a></li>
                {{/if}}
                {{#if_undefined user_name}}
                <li><a href="/login.html"><i class="icon-off"></i>登录</a></li>
                {{/if_undefined}}
              </ul>
              </li>
              <!-- end: User Dropdown -->
            </ul>
          </div>
