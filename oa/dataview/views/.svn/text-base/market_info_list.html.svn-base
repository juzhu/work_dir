<div class="row-fluid">
  <div class="box span12">
    <div class="box-header">
      <h2><i class="icon-align-justify"></i><span class="break"></span>营销内容</h2>
      <div class="box-icon">
        <a href="#" class="btn-minimize"><i class="icon-chevron-up"></i></a>
        <a href="#" class="btn-close"><i class="icon-remove"></i></a>
      </div>
    </div>
    <div class="box-content">
      <table class="table table-bordered table-striped table-condensed">
        <thead>
          <tr>
            <th>时间</th>
            <th>文案</th>
            <th>查询语句</th>
            <th>筛选数</th>
            <th>发送数</th>
            <th>成功</th>
            <th>黑名单</th>
            <th>其他失败</th>
            <th>两日内订单数</th>
            <th>操作</th>
          </tr>
        </thead>
        <tbody>
          {{#each sms_info}}
          <tr>
            <td class="center" style='width:10%'>{{day_str}}</td>
            <td class="center" style='width:40%'>{{content}}</td>
            <td class="center" style='width:10%'>{{sql_str}}</td>
            <td class="center"  style='width:5%'>{{phone_num}}</td>
            <td class="center"  style='width:5%'>{{total_send}}</td>
            <td class="center" style='width:5%'>{{success}}</td>
            <td class="center" style='width:5%'>{{blacklist}}</td>
            <td class="center" style='width:5%'>{{other_failed}}</td>
            <td class="center" style='width:5%'>{{order_num}}</td>
            <td class="center"  style='width:10%'><a href='/user_market/{{sms_id}}'>详情</a></td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div><!--/span-->
</div><!--/row-->

<script type="text/javascript">
</script>
